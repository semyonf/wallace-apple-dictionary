{
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:cov": "jest --coverage",
    "postinstall": "rm -rf .git/hooks && ln -s $(pwd)/git-hooks .git/hooks"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "os": [
    "darwin"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=16 <17"
  },
  "private": true,
  "dependencies": {
    "axios": "0.24.0",
    "jsdom": "19.0.0",
    "p-queue": "6.6.2",
    "reflect-metadata": "0.1.13",
    "snake-case": "3.0.3",
    "tsyringe": "4.6.0",
    "xml": "1.0.1"
  },
  "devDependencies": {
    "@tsconfig/node16-strictest": "1.0.1",
    "@types/jest": "27.0.3",
    "@types/jsdom": "16.2.13",
    "@types/node": "16.11.27",
    "@types/xml": "1.0.8",
    "@types/xml2js": "0.4.9",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "eslint": "7.17.0",
    "jest": "27.4.3",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "ts-jest": "27.0.7",
    "ts-node": "8.10.2",
    "typescript": "4.5.2",
    "xml2js": "0.4.23"
  }
